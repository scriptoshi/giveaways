{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "cyrildewit/eloquent-viewable": "^7.0",
        "doctrine/dbal": "^3.5",
        "envatic/laravel-vue-i18n-generator": "^0.1.50",
        "guzzlehttp/guzzle": "^7.2",
        "iksaku/laravel-mass-update": "^1.0",
        "inertiajs/inertia-laravel": "^0.6.9",
        "ixudra/curl": "^6.22",
        "jackiedo/dotenv-editor": "^2.1",
        "kkomelin/laravel-translatable-string-exporter": "^1.16",
        "kornrunner/ethereum-address": "^0.3.0",
        "kornrunner/ethereum-util": "^0.1.0",
        "kornrunner/secp256k1": "^0.2.0",
        "kornrunner/solidity": "^0.2.0",
        "krowinski/bcmath-extended": "^6.0",
        "laravel-notification-channels/pusher-push-notifications": "^4.2",
        "laravel-notification-channels/telegram": "^5.0",
        "laravel/framework": "^10.0",
        "laravel/jetstream": "^2.9",
        "laravel/sanctum": "^3.2",
        "laravel/scout": "^10.9",
        "laravel/socialite": "^5.12",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "mavinoo/laravel-batch": "^2.3",
        "psr/simple-cache": "^2.0",
        "qirolab/laravel-reactions": "^2.8",
        "rinvex/countries": "^9.0",
        "rtconner/laravel-likeable": "^3.2",
        "simplito/elliptic-php": "^1.0",
        "sleepfinance/eip712": "^1.0",
        "sleepfinance/simple-web3-php": "^0.13.0",
        "sleepfinance/web3.php": "^0.1.8",
        "socialiteproviders/discord": "^4.2",
        "socialiteproviders/telegram": "^4.1",
        "sopamo/laravel-filepond": "^1.2",
        "symfony/http-client": "^6.1",
        "symfony/mailgun-mailer": "^6.1",
        "symfony/postmark-mailer": "^6.0",
        "tightenco/ziggy": "^1.0",
        "watson/sitemap": "^5.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.12",
        "envatic/crud-strap": "^4.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
