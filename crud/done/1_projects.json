            "class": "Quester|Giveaway::class"
        },
        {
            "name": "nfts",
            "type": "hasManyThrough",
            "class": "Nft|Giveaway::class"
        },
        {
            "name": "launchpad",
            "type": "hasOne",
            "class": "Launchpad|launchpad_id|id"
        },
        {
            "name": "uploads",
            "type": "morphMany",
            "class": "Upload|uploadable"
        },
        {
            "name": "logo",
            "type": "morphOne|where:'key','logo'",
            "class": "Upload|uploadable"
        }
    ]
}