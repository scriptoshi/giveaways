        },
        {
            "name": "uuid",
            "type": "uuid"
        },
        {
            "name": "name",
            "type": "string",
            "rules": "required|string"
        },
        {
            "name": "slug",
            "type": "string",
            "rules": "required|string"
        },
        {
            "name": "description",
            "type": "text|nullable",
            "rules": "required|string"
        },
        {
            "name": "rank",
            "type": "integer",
            "rules": "required|string"
        },
        {
            "name": "verified_at",
            "type": "timestamp|nullable"
        },
        {
            "name": "promoted_at",
            "type": "timestamp|nullable"
        }
    ],
    "relationships": [
        {
